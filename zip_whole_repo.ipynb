    "\n",
    "for dirname, subdirs, files in os.walk(\"../2018-12-22_seqGan-music\"):\n",
    "    zf.write(dirname)\n",
    "    for filename in files:\n",
    "        zf.write(os.path.join(dirname, filename))\n",
    "zf.close()\n",
